<?xml version="1.0" ?>
<sdf version="1.4">
  <world name="default">
    <plugin name="ros_link_attacher_plugin" filename="libgazebo_ros_link_attacher.so"/>
    <plugin name="follow_object" filename="libfollow_object.so"/>
    <plugin name="goal_out_detection" filename="libgoal_out_detection.so"/>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <!-- A global light source-->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- RoboCup SPL 2014 field -->
    <include>
      <uri>model://robocup_ssl_field</uri>
    </include>
    <!-- RoboCup SPL 2014 left goal -->
    <include>
      <pose>-4.5 0 0 0 0 3.1416</pose>
      <uri>model://robocup_ssl_goal</uri>
      <name>robocup_ssl_left_goal</name>
    </include>
    <!-- RoboCup SPL 2014 right goal -->
    <include>
      <pose>4.5 0 0 0 0 0</pose>
      <uri>model://robocup_ssl_goal</uri>
      <name>robocup_ssl_right_goal</name>
    </include>
    <!-- RoboCup SPL ball -->
    <include>
      <pose>0 0 0.0315 0 0 0</pose>
      <uri>model://robocup_ssl_ball</uri>
      <name>ball</name>
      <plugin name="ball_controller" filename="libball_controller.so"/>
    </include>
    <!-- Turtlebot 1 -->
    <include>
      <pose>4 2 0.01 0 0 0</pose>
      <uri>model://turtlebot</uri>
      <name>turtlebot1</name>
      <!-- <plugin name="model_push" filename="libmodel_push.so"/> -->
      <plugin name="set_orient_and_chase" filename="libset_orient_and_chase.so"/>
    </include>
    <!-- Turtlebot 2 -->
    <include>
      <pose>0 1 0.01 0 0 0</pose>
      <uri>model://turtlebot</uri>
      <name>turtlebot2</name>
      <plugin name="move_to_goal" filename="libmove_to_goal.so"/>
    </include>
    <!-- Turtlebot 3 -->
    <include>
      <pose>-3 -1 0.01 0 0 -1.52</pose>
      <uri>model://turtlebot</uri>
      <name>turtlebot3</name>
      <plugin name="make_goal" filename="libmake_goal.so"/>
    </include>
  </world>
</sdf>